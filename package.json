{
  "name": "chessington",
  "version": "1.0.0",
  "description": "Starter project for a chess-based TDD exercise",
  "main": "src/index.js",
  "scripts": {
    "clean": "del-cli dist/**/* logs/*",
    "bundle": "webpack",
    "check-types": "tsc",
    "build": "npm run check-types && npm run clean && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files && npm run bundle",
    "start": "npm run build && node dist/index.js",
    "test": "mocha  --require ts-node/register \"./tests/**/*.spec.ts\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/chai": "^4.3.3",
    "@types/jest": "^29.1.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.1.2",
    "del-cli": "^1.1.0",
    "mocha": "^4.0.1",
    "source-map-loader": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "express": "^4.16.1"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MariaDanielaNegrut/chessington-ts.git"
  },
  "bugs": {
    "url": "https://github.com/MariaDanielaNegrut/chessington-ts/issues"
  },
  "homepage": "https://github.com/MariaDanielaNegrut/chessington-ts#readme"
}
